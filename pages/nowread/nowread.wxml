<view class="contain">
  <view class="artical" wx:for="{{nowRead}}" wx:key="key" wx:for-item="item">
    <image class="aimg" src="{{item.img}}"></image>
    <view class="desc">
      <view class="title">{{item.storyName}}</view>
      <text class="bookmark fa fa-bookmark">阅读至第{{item.nchapter}}</text>
    </view>
  </view>

</view>